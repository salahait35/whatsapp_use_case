{
  "routes": [
    {
      "route": "/home.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/login.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/",
      "redirect": "/login.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": 302
    },
    {
      "route": "/.auth/logout",
      "statusCode": 302
    }
  ],
  "navigationFallback": {
    "rewrite": "/login.html",
    "exclude": ["/.auth/*"]
  }
}
