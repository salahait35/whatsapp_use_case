{
    "routes": [
      {
        "route": "/inscription",
        "allowedRoles": ["anonymous"],  
        "rewrite": "/inscription.html"
      },
      {
        "route": "/.auth/login/aad",
        "statusCode": 302
      }
    ],
    "auth": {
      "identityProviders": {
        "azureActiveDirectory": {
          "registration": {
            "clientId": "3ef5aac4-dabf-40ad-b133-e72dce761112",
            "clientSecretSettingName": "SECRET"
          }
        }
      }
    },
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/.auth/*"]
    }
  }
  